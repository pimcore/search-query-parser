{
  "name": "pimcore/search-query-parser",
  "description": "Parses a simplified SQL-like query string into a syntax tree which can be used to build search queries",
  "license": "GPL-3.0",
  "require": {
    "php": ">=7.0",
    "nikic/phlexy": "^0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "symfony/var-dumper": "^3.2",
    "zendframework/zendframework1": "^1.12"
  },
  "suggest": {
    "zendframework/zendframework1": "If using the Zend_Db query builder"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "SearchQueryParser\\": "src/"
    },
    "exclude-from-classmap": ["examples"]
  },
  "autoload-dev": {
    "psr-4": {
      "SearchQueryParser\\Test\\": "test/"
    }
  }
}
